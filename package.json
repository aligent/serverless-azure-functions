{
  "name": "serverless-azure-functions",
  "version": "2.1.3",
  "description": "Provider plugin for the Serverless Framework v1.x which adds support for Azure Functions.",
  "license": "MIT",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "author": "Azure Functions",
  "scripts": {
    "start": "npm run test -- --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "pretest": "npm run lint",
    "test": "jest",
    "test:ci": "npm run test -- --ci",
    "test:coverage": "npm run test -- --coverage",
    "clean:int": "rm -rf integrationTests/configurations/**/.serverless integrationTests/configurations/**/node_modules integrationTests/configurations/**/local.settings.json",
    "pretest:int": "npm run clean:int",
    "test:int": "clvr",
    "compile": "tsc",
    "prebuild": "shx rm -rf lib/ && npm run test",
    "build": "npm run compile",
    "generate:spn": "bash ./scripts/generate-service-principal.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:condensed": "npm run changelog && node ./scripts/condenseChangelog.js"
  },
  "repository": {
    "git": "https://github.com/serverless/serverless-azure-functions"
  },
  "lcov.path": [
    "./coverage/lcov.info"
  ],
  "homepage": "https://github.com/serverless/serverless-azure-functions",
  "keywords": [
    "serverless",
    "serverless framework",
    "serverless applications",
    "serverless modules",
    "azure functions",
    "iot",
    "internet of things",
    "serverless.com"
  ],
  "files": [
    "lib/"
  ],
  "dependencies": {
    "@azure/arm-apimanagement": "^8.0.1",
    "@azure/arm-appservice": "^13.0.0",
    "@azure/arm-keyvault": "^2.0.0",
    "@azure/arm-resources": "^5.0.1",
    "@azure/arm-storage": "^18.0.0",
    "@azure/ms-rest-nodeauth": "^3.1.1",
    "@azure/storage-blob": "^12.11.0",
    "acorn": "^8.8.0",
    "axios": "^0.27.2",
    "azure-functions-core-tools": "^4.0.4736",
    "cross-spawn": "^7.0.3",
    "deep-equal": "^2.0.5",
    "js-yaml": "^4.1.0",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "open": "^8.4.0",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "unzipper": "^0.10.11"
    "xml2js": "^0.4.23",
    "zip-folder": "^1.0.0"
  },
  "devDependencies": {
    "@azure/ms-rest-js": "^2.6.2",
    "@babel/runtime": "^7.18.9",
    "@types/jest": "^29.0.0",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.184",
    "@types/mock-fs": "^4.13.1",
    "@types/open": "^6.1.0",
    "@types/request": "^2.48.1",
    "@types/serverless": "^1.18.2",
    "@types/xml": "^1.0.3",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-jest": "^24.8.0",
    "babel-preset-react-app": "^9.0.0",
    "clvr": "^0.7.1",
    "conventional-changelog-cli": "^2.0.23",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "jest-os-detection": "^1.1.1",
    "mock-fs": "^4.10.0",
    "mock-spawn": "^0.2.6",
    "serverless": "^3.22.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "serverless-webpack": "^4.2.0",
    "webpack": "^3.10.0"
  },
  "engines": {
    "npm" : ">=7.0.0",
    "node" : "~16.11.0"
  }
}
